    "def ScrapeAll(url, df, category):\n",
    "    # Input: list containing result urls, df with set column names\n",
    "    # output: dataframe containing features\n",
    "    \n",
    "    # change useragent\n",
    "#DEBUG\n",
    "#url = 'https://rent.591.com.tw/rent-detail-5253790.html'\n",
    "   \n",
    "    req = urllib.request.Request(\n",
    "        url, \n",
    "        data=None, \n",
    "        headers={\n",
    "            'User-Agent': 'Mozilla/5.0'\n",
    "        }\n",
    "    )\n",
    "\n",
    "    # now return the html\n",
    "    html = urllib.request.urlopen(req)\n",
    "    \n",
    "    \n",
    "\n",
    "    # find all links of the results, these are the rentals we are looking for\n",
    "    sp = BeautifulSoup(html, 'html.parser')\n",
    "    \n",
    "    # price: nested like:\n",
    "    # <div class=\"price clearfix\"><i>15,900 <b>元/月</b> </i></div>\n",
    "    priceList = sp.find_all(class_ = 'price clearfix')[0].contents[1].contents \n",
    "    \n",
    "    price = int((priceList[0].replace(',','')).replace(' ',''))# gets 15,900 as int\n",
    "    unit = str(priceList[1].string)\n",
    "    \n",
    "    \n",
    "    # title of the rental: single layer\n",
    "    name = str(sp.find_all(class_ = 'houseInfoTitle')[0].contents[0])\n",
    "    \n",
    "    # address: single layer\n",
    "    addr = str(sp.find_all(class_ = 'addr')[0].contents[0])\n",
    "    \n",
    "    # size, floor, 型態, 現況: nested like\n",
    "    #`<ul class=\"attr\"> <li>坪數 :  9坪</li><li>樓層 :  6F/14F</li><li>型態 :  住宅大樓</li><li>現況 :  分租套房</li> </ul>`\n",
    "    footage = sp.find_all(class_ = 'attr')[0].contents \n",
    "    \n",
    "    size = floor = typef = cond = on_roof = np.nan\n",
    "    for i in range(len(footage)):\n",
    "        if '坪數' in str(footage[i].string):\n",
    "            size = str(footage[i].string)\n",
    "            size = float(size.split(':')[1].replace('坪','').replace(' ','').replace(u'\\xa0', ''))\n",
    "        elif '樓層' in str(footage[i].string):\n",
    "            floor = str(footage[i].string)\n",
    "            floor = floor.split(':')[1]\n",
    "            floor = floor.split('/')\n",
    "            for j in floor:\n",
    "                if 'F' in j:\n",
    "                    floor = int(j.replace('F','').replace(' ','').replace(u'\\xa0', ''))\n",
    "            if '頂樓加蓋' in str(footage[i].string):\n",
    "                on_roof = True\n",
    "            else:\n",
    "                on_roof = False\n",
    "        elif '型態' in str(footage[i].string):\n",
    "            typef = str(footage[i].string)\n",
    "            typef = typef.split(':')[1].replace(' ','')\n",
    "        elif '現況' in str(footage[i].string):\n",
    "            cond = str(footage[i].string)\n",
    "            cond = cond = cond.split(':')[1].replace(' ','')\n",
    "            \n",
    "            \n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    # saving data\n",
    "    df.loc[url] = pd.Series({\n",
    "        'url':url, \n",
    "        'price':price, \n",
    "        'unit':unit, \n",
    "        'address':addr, \n",
    "        'size':size, \n",
    "        'floor':floor, \n",
    "        'type':typef, \n",
    "        'condition':cond, \n",
    "        'category':category,\n",
    "        'on_roof':on_roof,\n",
    "        'name' : name\n",
    "    })\n",
    "    \n",
    "    \n"
   ]
