    "# the dataframe to store everything\n",
    "df = pd.DataFrame(columns=['url','price','unit','address','size','floor','type','condition','category','name','on_roof'])\n",
    "\n",
    "url_share     = 'https://rent.591.com.tw/home/rent/index/r1s7k3.html?kind=1&region=1&section=7'\n",
    "for link in QueryToLink(url_share):\n",
    "    ScrapeAll(link, df, 'share')\n",
